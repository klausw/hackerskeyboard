<?xml version="1.0" encoding="utf-8"?>
<Keyboard
    android:horizontalGap="0.0px"
    android:keyWidth="16.666662%p"
    android:verticalGap="@dimen/key_bottom_gap"
    xmlns:android="http://schemas.android.com/apk/res-auto">
    <Row
        android:rowEdgeFlags="top">
        <Key
            android:isModifier="true"
            android:keyEdgeFlags="left"
            android:keyLabel="/"
            android:keyWidth="25.0%p" />
        <Key
            android:keyLabel="(" />
        <Key
            android:keyLabel="N" />
        <Key
            android:keyLabel=")" />
        <Key
            android:codes="@integer/key_delete"
            android:iconPreview="@drawable/sym_keyboard_feedback_delete"
            android:isModifier="true"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="25.0%p" />
    </Row>
    <Row>
        <Key
            android:isModifier="true"
            android:keyEdgeFlags="left"
            android:keyLabel="-"
            android:keyWidth="25.0%p" />
        <Key
            android:codes="44"
            android:keyLabel="Pause"
            android:keyWidth="25.0%p" />
        <Key
            android:codes="59"
            android:keyLabel="Wait"
            android:keyWidth="25.0%p" />
        <Key
            android:codes="@integer/key_return"
            android:isModifier="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="25.0%p" />
    </Row>
    <Row>
        <Key
            android:isModifier="true"
            android:keyEdgeFlags="left"
            android:keyLabel=","
            android:keyWidth="25.0%p" />
        <Key
            android:codes="42"
            android:keyLabel="*"
            android:keyWidth="25.0%p" />
        <Key
            android:codes="35"
            android:keyLabel="\#"
            android:keyWidth="25.0%p" />
        <Key
            android:isModifier="true"
            android:keyEdgeFlags="right"
            android:keyLabel="."
            android:keyWidth="25.0%p" />
    </Row>
    <Row
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/key_symbol"
            android:isModifier="true"
            android:keyEdgeFlags="left"
            android:keyLabel="@string/label_phone_key"
            android:keyWidth="41.666664%p" />
        <Key
            android:keyLabel="+" />
        <Key
            android:codes="@integer/key_space"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isModifier="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="41.666664%p" />
    </Row>
</Keyboard>